#%RAML 1.0 Library

types:

  UnauthorizedResponse:
    displayName: 401 Response
    type: object
    properties:
      status:
        type: string
        enum:
          - unauthorized
      message: string
    additionalProperties: false

  ServerErrorResponse:
    displayName: 500 Response
    type: object
    properties:
      status:
        type: string
        enum:
          - server-error
        required: true
      message: string
    additionalProperties: false

